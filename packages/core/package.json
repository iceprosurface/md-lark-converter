{
  "name": "@md-lark-converter/core",
  "version": "1.0.1",
  "description": "Core conversion logic for Markdown to Lark",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "require": "./dist/node.js",
      "import": "./dist/node.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "pnpm build",
    "dev": "tsup --watch",
    "test": "vitest --run"
  },
  "keywords": [
    "markdown",
    "lark",
    "feishu",
    "converter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iceprosurface/md-lark-converter.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/iceprosurface/md-lark-converter/issues"
  },
  "homepage": "https://github.com/iceprosurface/md-lark-converter#readme",
  "author": "icepro",
  "license": "MIT",
  "dependencies": {
    "marked": "^14.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/marked": "^6.0.0",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.0.0",
    "mdast-util-math": "^3.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vitest": "^2.1.9"
  }
}
